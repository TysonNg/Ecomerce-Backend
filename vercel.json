{
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "/api" }],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
      }
    ]
  }